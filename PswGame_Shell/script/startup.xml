<?xml version="1.0" encoding="UTF-8"?>
<startup name="startup">
	<!-- 
	字段解释：
	type为模块类型（exe为可执行程序，加载完成后将自动执行，dll为库,考虑后面用int作为其值类型）
	name为模块名称
	url为模块路径
	domain为模块所在域 0为系统域，1为游戏域，2为地图域
	useSub指示是否用所在域的子域  
	 -->
<!--	<item name="components" type="dll" url="dlls/components.swf">-->
<!--		<domain level="1" useSub="0"/>-->
<!--	</item>-->
	<item name="system" type="dll" url="dlls/system.swf">
		<domain level="1" useSub="0"/>
	</item>
	<item name="login" type="exe" url="plugins/LoginPlugin.swf">
		<domain level="1" useSub="1" name="login"/>
	</item>
</startup>
